define("s-image-edit/utils/photo-filters",["exports","ember-stdlib/utils/environment","ember"],function(e,t,n){function a(e){return e<0?0:e>255?255:e}function i(e){for(var t=e.length,n=new Array(t),a=0;a<t;a++)n[a]=new Array(3)
var i=[]
n[0][1]=1
n[0][0]=0
n[0][2]=0
for(var a=1;a<t-1;a++){var s=e[a-1],l=e[a],o=e[a+1]
n[a][0]=(l.x-s.x)/6
n[a][1]=(o.x-s.x)/3
n[a][2]=(o.x-l.x)/6
i[a]=(o.y-l.y)/(o.x-l.x)-(l.y-s.y)/(l.x-s.x)}i[0]=0
i[t-1]=0
n[t-1][1]=1
n[t-1][0]=0
n[t-1][2]=0
for(var a=1;a<t;a++){var r=n[a][0]/n[a-1][1]
n[a][1]-=r*n[a-1][2]
n[a][0]=0
i[a]-=r*i[a-1]}for(var a=t-2;a>=0;a--){var r=n[a][2]/n[a+1][1]
n[a][1]-=r*n[a+1][0]
n[a][2]=0
